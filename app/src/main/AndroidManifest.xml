<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.kkn1.mindahannewnormal">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <uses-feature android:name="android.hardware.screen.portrait" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".NointernetActivity"></activity>
        <activity android:name=".BagikanActivity" />
        <activity android:name=".WJeparaActivity" />
        <activity android:name=".WJatengActivity" />
        <activity android:name=".WNasActivity" />
        <activity android:name=".WebProsehatActivity" />
        <activity android:name=".WebHalodocActivity" />
        <activity android:name=".WebPrixaActivity" />
        <activity android:name=".WebJatengActivity" />
        <activity android:name=".PeriksaActivity" />
        <activity android:name=".AboutActivity" />
        <activity android:name=".item.itemfaq"
            tools:ignore="Instantiatable" />
        <activity android:name=".item.itemtips"
            tools:ignore="Instantiatable" />
        <activity android:name=".FaqActivity" />
        <activity android:name=".TipsActivity" />
        <activity
            android:name=".SplasScreenActivity"
            android:theme="@style/FullscreenTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MyIntroActivity"
            android:screenOrientation="portrait"
            android:theme="@style/FullscreenTheme" />
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait"
            android:theme="@style/FullscreenTheme" />
    </application>

</manifest>